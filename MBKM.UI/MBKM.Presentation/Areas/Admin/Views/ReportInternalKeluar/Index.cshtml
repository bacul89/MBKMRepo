
@{
    ViewBag.Title = "REPORT MBKM PERTUKARAN INTERNAL KE LUAR";
    Layout = "~/Areas/Admin/Views/Shared/_Layout2.cshtml";
}
@section css{

    <link rel="stylesheet" type="text/css" href="~/Content/Admin/inputResponsive.css">
    @Scripts.Render("~/bundles/jquery")
    <script src="~/Scripts/sweetalert2.all.min.js"></script>
    <script src="https://cdn.datatables.net/1.11.3/css/jquery.dataTables.min.css"></script>
    <script src="https://cdn.datatables.net/buttons/2.0.1/css/buttons.dataTables.min.css"></script>
}
<br />
<div class="container-fluid" style="padding: 0px">
    <div class="card center-block" style="background-color: white; width: 98%; padding: 0px">
        @{

            <div class="card-body responsive-content" style="box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2); border: 3px solid coral; padding: 0px">
                <h6 class="card-title" style="padding-top: 30px; padding-right: 20px; padding-left: 30px; font-weight: bold; font-size: 25px; color: black;">
                    REPORT MBKM PERTUKARAN INTERNAL KE LUAR ATMA JAYA<br />

                </h6>
                <hr />
                <div class="container" style="width: 100%;">

                    <div class="col-md-12">
                        <form class="form-horizontal col-md-5">
                            <div class="form-group">
                                <label for="inp_semester" class="col-sm-3 control-label" style="margin-left:-34px;width:30%">Tahun Semester</label>
                                <div class="col-sm-7">
                                    <select name="inp_semester" id="inp_semester" class="form-control select2 input-md input-data">
                                        <option value="" selected disabled>-- Pilih Tahun Semester --</option>
                                        @{
                                            foreach (var dt in ViewData["semester"] as IEnumerable<MBKM.Entities.ViewModel.VMSemester>)
                                            {
                                                <option value="@dt.Nilai">@dt.Nama</option>
                                            }
                                        }
                                    </select>
                                </div>
                            </div>
                        </form>
                    </div>
                    <!--table-responsive-->
                    <table id="table-data-report-internal-mahasiswa" class="table row-border hover dt-responsive responsive" style="border-spacing: 0 5px; border:0">
                        <thead>
                            <tr>
                                <th rowspan="2" class="zero-padding2 no-border small-text vertical-center center ">No</th>
                                <th rowspan="2" class="zero-padding2 no-border small-text vertical-center center ">Tahun Semester</th>
                                <th rowspan="2" class="zero-padding2 no-border small-text vertical-center center ">Jenjang Studi</th>
                                <th rowspan="2" class="zero-padding2 no-border small-text vertical-center center ">NIM</th>
                                <th rowspan="2" class="zero-padding2 no-border small-text vertical-center center ">Nama Mahasiswa</th>
                                <th rowspan="2" class="zero-padding2 no-border small-text vertical-center center ">Program Studi</th>
                                <th colspan="8" class="zero-padding2 no-border small-text vertical-center center ">Nilai Transfer</th>
                            </tr>
                            <tr>
                                <th class="zero-padding2 no-border small-text vertical-center center ">Kampus Tempat MBKM</th>
                                <th class="zero-padding2 no-border small-text vertical-center center ">Nama Mata Kuliah Asal </th>
                                <th class="zero-padding2 no-border small-text vertical-center center ">SKS</th>
                                <th class="zero-padding2 no-border small-text vertical-center center ">Nilai Huruf Asal</th>
                                <th class="zero-padding2 no-border small-text vertical-center center ">Kode Mata kuliah Diakui</th>
                                <th class="zero-padding2 no-border small-text vertical-center center ">Nama Mata Kuliah Diakui</th>
                                <th class="zero-padding2 no-border small-text vertical-center center ">Nilai Huruf Diakui</th>
                                <th class="zero-padding2 no-border small-text vertical-center center ">Bobot Nilai</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                    <br />
                    <br />
                    <br />
                    <br />
                </div>
            </div>
        }




    </div>
</div>

@section scripts{
    <script src="~/Scripts/Admin/SummaryFeedback/main.js"></script>
    <script src="~/Scripts/Admin/ReportMBKMPertukaranInternalKeluar/main.js"></script>
    <script src="~/Scripts/loading/loadingoverlay.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.js"> </script>
    <script src="https://cdn.datatables.net/1.11.3/js/jquery.dataTables.min.js"> </script>
    <script src="https://cdn.datatables.net/buttons/2.0.1/js/dataTables.buttons.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"> </script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"> </script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"> </script>
    <script src="https://cdn.datatables.net/buttons/2.0.1/js/buttons.html5.min.js"> </script>
    <script src="https://cdn.datatables.net/buttons/2.0.1/js/buttons.print.min.js"></script>
}

