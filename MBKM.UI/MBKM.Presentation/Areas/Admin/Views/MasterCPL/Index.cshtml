<div class="container-fluid" style="padding: 0px">
    <div class="card center-block" style="background-color: white; width: 100%">
        <div class="card-body" style="box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2); border:3px solid coral">
            <h4 class="card-title" style="padding-top: 30px; padding-right: 20px; padding-left: 30px; font-weight: bold; font-size: 30px; color: black;">
                MANAGE CAPAIAN PEMBELAJARAN
            </h4>
            <hr />
            <div class="container" style="width: 100%">
                <div class="col-md-12">
                    <form class="form-horizontal col-md-7">

                        <div class="form-group">
                            <label for="inp_jenjang" class="col-sm-3 control-label">Jenjang Studi</label>
                            <div class="col-sm-7">
                                <select name="inp_jenjang" id="inp_jenjang" class="form-control input-md input-data">
                                    <option value="" selected disabled>-- Pilih Jenjang Studi --</option>
                                    @{
                                        foreach (var d in ViewData["Jenjang"] as IEnumerable<MBKM.Entities.ViewModel.VMLookup>)
                                        {
                                            <option value="@d.Nilai">@d.Nama</option>
                                        }
                                    }
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inp_fakultas" class="col-sm-3 control-label">Fakultas</label>
                            <div class="col-sm-7">
                                <select name="inp_fakultas" id="inp_fakultas" class="form-control input-md input-data">
                                    <option value="" selected disabled>-- Pilih Fakultas --</option>
                                </select>
                            </div>
                        </div> 
                        <div class="form-group">
                            <label for="inp_prodi" class="col-sm-3 control-label">Prodi</label>
                            <div class="col-sm-7">
                                <select name="inp_prodi" id="inp_prodi" class="form-control input-md input-data">
                                    <option value="" selected disabled>-- Pilih Prodi --</option>
                                </select>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-2 col-md-offset-8">
                                <button type="button" id="cari" class="btn btn-warning" style="float: right;" onclick="GenerateTable()"><span class="fa fa-search"></span> Cari</button>
                            </div>
                        </div>
                    </form>
                </div>

                <button data-toggle="modal" data-target="#TambahCPL" class="btn btn-success"><i class="fas fa-plus-circle"></i> Tambah CPL</button>
                <br />
                <br />
                <table id="TableList" class="table table-bordered table-striped hover dt-responsive responsive">
                    <thead class="text-center">
                        <tr style="background-color: #FF8A04; border: 0 ">
                            <th class="zero-padding no-border vertical-center sorting" style="text-align:center">Action</th>
                            <th class="zero-padding no-border vertical-center sorting" style="text-align:center">No.</th>
                            <th class="zero-padding no-border vertical-center sorting" style="text-align:center">Kode</th>
                            <th class="zero-padding no-border vertical-center sorting" style="text-align:center">Kelompok</th>
                            <th class="zero-padding no-border vertical-center sorting" style="text-align:center">Capaian Pembelajaran</th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

@section scripts{

    <script src="~/Scripts/loading/loadingoverlay.min.js"></script>
    <script src="~/Scripts/select2.min.js"></script>
    <script>
        var tableUser = null;
        function clearValueOnForm() {
            $("#prodi").val('');
            $("#fakultas").val('');
            $("#jenjang").val('');
            $('input[name=inp_status]').prop('checked', false);
        }
        $(document).ready(function () {
            loadFakultas();
            //loadFakultasEdit();
            loadFromLookup("JenjangStudi", "jenjang", "JenjangStudi");
            //loadFromLookup("JenjangStudi", "jenjangEdit", "JenjangStudi");
            loadFromLookup("Kelompok", "kelompok", "Kelompok");
            /*loadFromLookup("Kelompok", "abc", "Kelompok");*/
            tableUser = $('#TableList').DataTable({});
            
            $("#prodi").select2({
                //dropdownParent: $("#TambahCPL"),
                placeholder: "-- Pilih Program Studi --"
            });
            $("#lokasi").select2({
                //dropdownParent: $("#TambahCPL"),
                placeholder: "-- Pilih Lokasi --"
            });


            $('#inp_jenjang').change(function () {
                $("#inp_fakultas").prop('selectedIndex', 0);
                $("#inp_prodi").prop('selectedIndex', 0);
            })

            $('#inp_jenjang').change(function () {
                $("#inp_fakultas").select2({
                    placeholder: "-- Pilih Fakultas --",
                    width: "100%",
                    ajax: {
                        url: "/Admin/MasterCPL/GetFakultas",
                        dataType: 'json',
                        method: "POST",
                        delay: 250,
                        cache: false,
                        data: function (params) {
                            return {
                                search: params.term || "",
                                jenjang: $('#inp_jenjang :selected').val(),
                            };
                        },
                        processResults: function (data, params) {
                            return {
                                results: $.map(data, function (item) { return { id: item.ID, value: item.ID, text: item.Nama } })
                            };
                        },
                    }
                });
            })
            $('#inp_fakultas').change(function () {
                $("#inp_prodi").select2({
                    placeholder: "-- Pilih Prodi --",
                    width: "100%",
                    ajax: {
                        url: "/Admin/MasterCPL/GetProdiByFakultas",
                        dataType: 'json',
                        method: "POST",
                        delay: 250,
                        cache: false,
                        data: function (params) {
                            return {
                                search: params.term || "",
                                idFakultas: $('#inp_fakultas :selected').val(),
                                jenjang: $('#inp_jenjang :selected').val(),
                            };
                        },
                        processResults: function (data, params) {
                            return {
                                results: $.map(data, function (item) { return { id: item.Nama, value: item.Nama, text: item.Nama } })
                            };
                        },
                    }
                });
            })

        });
        function loadFakultas() {
            $("#prodi").prop("disabled", true);
            $("#prodi2").prop("disabled", true);
            $("#lokasi").prop("disabled", true);
            $("#fakultas").prop("disabled", true);

            $("#fakultas").select2({
                multiple: false,
                placeholder: "-- Pilih Fakultas --",
                width: "100%",
                ajax: {
                    url: "@Url.Action("GetFakultas", "MasterCPL")",
                    dataType: 'json',
                    method: "POST",
                    delay: 250,
                    cache: false,
                    data: function (params) {
                        return {
                            Search: params.term || "",
                            Jenjang: $('#jenjang').val()
                        };
                    },
                    processResults: function (data, params) {
                        return {
                            results: $.map(data, function (item) { return { id: item.ID, value: item.ID, text: item.Nama } })
                        };
                    },
                }
            });
            $('#fakultas').on('select2:select', function (e) {
                console.log(e.params.data.text);
            });
            $("#jenjang").change(function () {
                $("#fakultas").empty();
                $("#fakultas").prop("disabled",false);
                $("#lokasi").empty();
                $("#lokasi").prop("disabled", true);
                $("#prodi").empty();
                $("#prodi2").empty();
                $("#prodi").prop("disabled", true);
                $("#prodi2").prop("disabled", true);
                //$("#lokasi").prop("disabled", true);
            });
            $("#fakultas").change(function () {
                //$('#fakultas').val(null).trigger('change');
                var latest_value = $("option:selected:first", this).text();
                //alert(latest_value);
                $("#prodi2").empty();
                $("#prodi").empty();
                $("#prodi2").prop("disabled", false);
                $("#prodi").prop("disabled", false);
                $("#lokasi").prop("disabled", true);
                $("#lokasi").empty();
                //-----------------------------------------------------------------------------
                $("#prodi2").select2({
                    placeholder: "-- Pilih Program Studi --",
                    width: "100%",
                    ajax: {
                        url: "/Admin/MasterCPL/GetProdiLocByFakultas",
                        dataType: 'json',
                        method: "POST",
                        delay: 250,
                        cache: false,
                        data: function (params) {
                            return {
                                Search: params.term || "",
                                //JenjangStudi: $('#' + id + "Cari").val(),
                                JenjangStudi: $('#jenjang').val(),
                                IdFakultas: $('#fakultas').val()
                            };
                        },
                        processResults: function (data, params) {
                            return {
                                results: $.map(data, function (item) { return { id: item.IDProdi, value: item.NamProdi, text: item.NamProdi + '-' + item.Lokasi, lokasi: item.Lokasi } })
                            };
                        },
                    }
                });
                //============================================================================================
                $("#prodi").select2({
                    multiple: false,
                    placeholder: "-- Pilih Program Studi --",
                    width: "100%",
                    ajax: {
                        url: "@Url.Action("GetProdiByFakultas", "MasterCPL")",
                        dataType: 'json',
                        method: "POST",
                        delay: 250,
                        cache: false,
                        data: function (params) {
                            return {
                                Search: params.term || "",
                                IDFakultas: $('#fakultas').val(),
                                Jenjang: $('#jenjang').val()
                            };
                        },
                        processResults: function (data, params) {
                            return {
                                results: $.map(data, function (item) { return { id: item.Nama, value: item.Nama, text: item.Nama } })

                            };
                        },
                    }
                });
                $("#prodi").change(function () {
                    $("#lokasi").empty();


                    $("#lokasi").prop("disabled",false);

                    $("#lokasi").select2({
                        placeholder: "-- Pilih Lokasi --",
                        width: "100%",
                        ajax: {
                            url: "@Url.Action("GetLokasiByProdi", "MasterCPL")",
                            dataType: 'json',
                            method: "POST",
                            delay: 250,
                            cache: false,
                            data: function (params) {
                                return {
                                    Search: params.term || "",
                                    NamaProdi: $('#prodi').val(),
                                    Jenjang: $('#jenjang').val()
                                };
                            },
                            processResults: function (data, params) {
                                return {
                                    results: $.map(data, function (item) { return { id: item.ID, value: item.ID, text: item.Kampus } })
                                };
                            },
                        }
                    });
                    
                });
                $('#lokasi').on('select2:select', function (e) {
                    console.log(e.params.data.id);
                    //alert(e.params.data.id)
                });
            });
        }

        //batas crud detail
        function DetailMasterCPL(id) {
            $.LoadingOverlay("show");
            $.ajax({
                url: "@Url.Action("ModalDetailMasterCpl", "MasterCPL")/"+id,
                type: 'get',
                datatype: 'html',
                success: function (data) {
                    $.LoadingOverlay("hide");
                    console.log(data);

                    $('#jenjangDetail').val(data.JenjangStudi);
