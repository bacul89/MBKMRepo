        //batas main
        function CustomValidation() {
            var isValid;
            $(".input-data :selected").each(function () {
                var element = $(this);
                if (element.val() == "") {
                    return isValid = false;
                } else {
                    return isValid = true;
                }
            });
            return isValid;
        }

