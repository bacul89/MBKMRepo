
@{
    ViewBag.Title = "Approval Daftar Mata Kuliah";
    Layout = "~/Areas/Admin/Views/Shared/_Layout2.cshtml";
}


@section css{
    <link rel="stylesheet" type="text/css" href="~/Content/Admin/inputResponsive.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Admin/AdminCostumeBS4.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Portal/TrackingStatus/main.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.11.1/css/jquery.dataTables.css">
    <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css" integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p" crossorigin="anonymous" />
}
<div class="container-fluid" style="padding:0px">
    <div class="card center-block" style="background-color: white; width: 100%">
        <div class="card-body" style="box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2); border:3px solid coral">
            <h4 class="card-title" style="padding-top: 30px; padding-right: 20px; padding-left: 30px; font-weight: bold; font-size: 30px; color: black;">
                DAFTAR MAHASISWA YANG PERLU DI APPROVED
            </h4>
            <hr />
            <div class="container-fluid">
                <br />
                <table id="table-data-approval-pendaftaran-makul" class="table row-border hover responsive" style="border-spacing: 0 15px; border:0">
                    <thead class="text-center">
                        <tr style="background-color: #FF8A04; border: 0 ">
                            <th class="zero-padding no-border vertical-center" style="text-align:center; font-size: 0.6vw">Isi CPL</th>
                            <th class="zero-padding no-border vertical-center" style="text-align:center; font-size: 0.6vw">No.</th>
                            <th class="zero-padding no-border vertical-center" style="text-align:center; font-size: 0.6vw">Universitas Asal</th>
                            <th class="zero-padding no-border vertical-center" style="text-align:center; font-size: 0.6vw">Program Studi Asal</th>
                            <th class="zero-padding no-border vertical-center" style="text-align:center; font-size: 0.6vw">Nim Asal</th>
                            <th class="zero-padding no-border vertical-center" style="text-align:center; font-size: 0.6vw">Nama Mahasiswa</th>
                            <th class="zero-padding no-border vertical-center" style="text-align:center; font-size: 0.6vw">Tahun Semester</th>
                            <th class="zero-padding no-border vertical-center" style="text-align:center; font-size: 0.6vw">Kode Mata Kuliah Asal</th>
                            <th class="zero-padding no-border vertical-center" style="text-align:center; font-size: 0.6vw">Nama Mata Kuliah Asal</th>
                            <th class="zero-padding no-border vertical-center" style="text-align:center; font-size: 0.6vw">Program Studi Tujuan</th>
                            <th class="zero-padding no-border vertical-center" style="text-align:center; font-size: 0.6vw">Kode Mata Kuliah Yang Dituju</th>
                            <th class="zero-padding no-border vertical-center" style="text-align:center; font-size: 0.6vw">Nama Mata Kuliah Yang Dituju</th>
                            <th class="zero-padding no-border vertical-center" style="text-align:center; font-size: 0.6vw">No. Kerjasama</th>
                        </tr>
                    </thead>
                    
                </table>
            </div>
        </div>
    </div>
</div>

@section scripts{
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.11.1/js/jquery.dataTables.js"></script>
    <script src="~/Scripts/Admin/ApprovalPendaftaranMakul/main.js"></script>
}

