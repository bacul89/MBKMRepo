            </div>
            <div class="form-group">
                <label for="inp_bodyEmail" class="col-sm-3 control-label">Kode Capian Pembelajaran</label>
                <div class="col-sm-9">
                    <select class="form-control input-md" name="inp_kode_capaian_pembelajaran" id="inp_kode_capaian_pembelajaran" placeholder="Pilih Kode Capian Pembelajaran">
                        <option value="@Model.MasterCapaianPembelajaranID" selected>@Model.MasterCapaianPembelajarans.Kode</option>
                    </select>
                </div>
            </div>

            <div class="form-group">
                <label for="inp_bodyEmail" class="col-sm-3 control-label">Capaian Pembelajaran</label>
                <div class="col-sm-9">
                    <textarea type="text" class="form-control input-pendaftaran input-data" name="inp_capaian" id="inp_capaian" placeholder="Keterangan" disabled>@Model.MasterCapaianPembelajarans.Capaian</textarea>
                </div>
            </div>
            <div class="form-group">
