                <div class="col-sm-9">
                    <input type="text" class="form-control input-md" name="inp_mata_kuliah" id="inp_mata_kuliah" placeholder="Pilih Mata Kuliah" disabled>
                </div>
            </div>
            <div class="form-group">
                <label for="inp_email" class="col-sm-3 control-label">Kelompok</label>
                <div class="col-sm-9">
                    <select class="form-control input-md" name="inp_kelompok" id="inp_kelompok" placeholder="Pilih Kelompok">
                        <option value="@Model.Kelompok" selected>@Model.Kelompok</option>
                    </select>
                    <!--<input type="text" >-->
