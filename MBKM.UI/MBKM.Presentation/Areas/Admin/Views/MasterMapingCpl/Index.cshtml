
@{
    ViewBag.Title = "MASTER MAPING CAPAIAN PEMBELAJARAN PER-MATA KULIAH";
    Layout = "~/Areas/Admin/Views/Shared/_Layout2.cshtml";
}

@section css{

    <link rel="stylesheet" type="text/css" href="~/Content/Admin/inputResponsive.css">
    @Scripts.Render("~/bundles/jquery")


    @Scripts.Render("~/bundles/jquery")
    <script src="~/Scripts/sweetalert2.all.min.js"></script>
}
@section modal{
    <div class="modal fade" id="TambahMasterMapingCPL" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document" id="modal-inner">

        </div>
    </div>
}

<div class="container-fluid" style="margin-bottom:100px;">
    <div class="card center-block" style="background-color: white; width: 100%; margin-top: 30px; margin-bottom: 50px;">
        <div class="card-body" style="box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2); border:3px solid coral">
            <h4 class="card-title" style="padding-top: 30px; padding-right: 20px; padding-left: 30px; font-weight: bold; font-size: 30px; color: black;">
                MASTER MAPING CAPAIAN PEMBELAJARAN PER-MATA KULIAH<br />
                <small class="describe">Berikut Table Mapping Capian Pembelajaran dengan Mata Kuliah</small>
            </h4>
            <hr />
            <div class="container" style="width: 100%;">

                <div class="col-md-12">
                    <form class="form-horizontal col-md-7">
                        <div class="form-group">
                            <label for="inp_tipeEmail" class="col-sm-3 control-label">Fakultas</label>
                            <div class="col-sm-9">
                                <select class="form-control input-pendaftaran input-data">
                                    <option value="value">-</option>
                                    <option value="value">hallo</option>
                                    <option value="value">hallo</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inp_email" class="col-sm-3 control-label">Program Studi</label>
                            <div class="col-sm-9">
                                <select class="form-control input-pendaftaran input-data">
                                    <option value="value">-</option>
                                    <option value="value">hallo</option>
                                    <option value="value">hallo</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inp_bodyEmail" class="col-sm-3 control-label">Mata Kuliah</label>
                            <div class="col-sm-9">
                                <select class="form-control input-pendaftaran input-data">
                                    <option value="value">-</option>
                                    <option value="value">hallo</option>
                                    <option value="value">hallo</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-2 col-md-offset-10">
                                <button type="button" onclick="SearchMasterLookup()" class="btn btn-warning">Cari</button>
                            </div>
                        </div>
                    </form>                   
                </div>



                <button class="btn btn-success" onclick="IndexCreateMasterMapingCpl()"><i class="fas fa-plus-circle"></i> Tambah Master Maping</button>
                <br />
                <br />
                <!--table-responsive-->
                <table id="table-data-master-mapping-cpl" class="table table-bordered table-spacing js-basic-example dataTable display nowrap">
                    <thead>
                        <tr>
                            <th class="zero-padding no-border vertical-center sorting">No.</th>
                            <th class="zero-padding no-border vertical-center sorting">Kode Mata Kuliah</th>
                            <th class="zero-padding no-border vertical-center sorting">Nama Mata Kuliah</th>
                            <th class="zero-padding no-border vertical-center sorting">Kode CPL</th>
                            <th class="zero-padding no-border vertical-center sorting">Kelompok</th>
                            <th class="zero-padding no-border vertical-center sorting">Capaian Pembelajaran</th>
                            <th class="zero-padding no-border vertical-center sorting">Action</th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>

            </div>
        </div>
    </div>
</div>

@section scripts{
    <script src="~/Scripts/Admin/MasterMapingCpl/main.js"></script>
    <script src="~/Scripts/Admin/MasterMapingCpl/crud.js"></script>
}

