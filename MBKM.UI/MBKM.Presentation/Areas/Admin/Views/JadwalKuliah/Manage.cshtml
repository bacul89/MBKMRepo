
@{
    ViewBag.Title = "MANAGE MATA KULIAH MBKM";
    Layout = "~/Areas/Admin/Views/Shared/_Layout2.cshtml";
}

@section css{

    <link rel="stylesheet" type="text/css" href="~/Content/Admin/inputResponsive.css">
    @Scripts.Render("~/bundles/jquery")


    @Scripts.Render("~/bundles/jquery")
    <script src="~/Scripts/sweetalert2.all.min.js"></script>
}

<div class="container-fluid" style="padding: 0px">
    <div class="card center-block" style="background-color: white; width: 100%; padding: 0px">
        <div class="card-body responsive-content" style="box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2); border: 3px solid coral; padding: 0px">
            <h4 class="card-title" style="padding-top: 30px; padding-right: 20px; padding-left: 30px; font-weight: bold; font-size: 30px; color: black;">
                MANAGE MATA KULIAH MBKM <br />
                <small class="describe">Manage Mata Kuliah yang akan dibuka untuk program MBKM</small>
            </h4>
            <hr />
            <div class="container" style="width: 100%;">

                <div class="col-md-12">
                    <form class="form-horizontal col-md-7">
                        <div class="form-group">
                            <label for="inp_tahunAjaran" class="col-sm-3 control-label">Tahun Semester</label>
                            <div class="col-sm-9">
                                <select name="tahunAjaranCari" id="tahunAjaranCari" class="form-control input-md" >
                                </select>
                            </div>
                        </div>

                        <div class="form-group">
                            <label for="inp_jenjang" class="col-sm-3 control-label">Jenjang Studi</label>
                            <div class="col-sm-9">
                                <select name="jenjangCari" id="jenjangCari" class="form-control input-md">
                                    @*<option value="" selected disabled>-- Pilih Authority Approval --</option>*@
                                </select>
                            </div>
                        </div>

                        <div class="form-group">
                            <label for="inp_fakultas" class="col-sm-3 control-label">Fakultas</label>
                            <div class="col-sm-9">
                                <select name="fakultasCari" id="fakultasCari" class="form-control input-md"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inp_prodi" class="col-sm-3 control-label">Program Studi</label>
                            <div class="col-sm-9">
                                <select name="prodiCari" id="prodiCari" class="form-control input-md"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inp_lokasi" class="col-sm-3 control-label">Lokasi</label>
                            <div class="col-sm-9">
                                <select name="lokasiCari" id="lokasiCari" class="form-control input-md"></select>
                            </div>
                        </div>
                        <div class="form-group" hidden>
                            <label for="inp_prodiId" class="col-sm-3 control-label">Kampus</label>
                            <div class="col-sm-9">
                                <input name="kampusCari" id="kampusCari" class="form-control input-md" disabled>
                            </div>
                        </div>


                        <div class="form-group" hidden>
                            <label for="inp_prodiId" class="col-sm-3 control-label">Prodi ID</label>
                            <div class="col-sm-9">
                                <input name="prodiIdCari" id="prodiIdCari" class="form-control input-md" disabled>
                            </div>
                        </div>


                        <div class="form-group">
                            <div class="col-sm-2 col-md-offset-10">
                                <button type="button" id="cari" class="btn btn-warning" style="float: right;" title="Mohon untuk melengkapi form pencarian terlebih dahulu!" disabled><span class="fa fa-search"></span> Cari</button>
                            </div>
                        </div>
                    </form>
                </div>




                <!--
                    No
                    Kode Mata Kuliah
                    Nama Mata Kuliah
                    SKS
                    Seksi
                    Hari
                    Waktu
                    Ruang Kelas
                    Lokasi
                    Nama Dosen
                -->
                <!--table-responsive-->
                <h3><b>JADWAL MATA KULIAH</b></h3>
                <table id="table-data-jadwal-kuliah" class="table table-bordered table-spacing js-basic-example dataTable display nowrap">
                    <thead>
                        <tr>
                            <th class="zero-padding no-border vertical-center sorting">No.</th>
                            <th class="zero-padding no-border vertical-center sorting">Kode Mata Kuliah</th>
                            <th class="zero-padding no-border vertical-center sorting">Mata Kuliah</th>
                            <th class="zero-padding no-border vertical-center sorting">SKS</th>
                            <th class="zero-padding no-border vertical-center sorting">Seksi</th>
                            <th class="zero-padding no-border vertical-center sorting">MBKM</th>

                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>

                <div class="col-lg-12">
                    <div class="form-group">
                        <div class="col-sm-2 col-md-offset-10">
                            <button type="button" id="add" class="btn btn-warning" style="float: right; padding-right: 20px; margin-top: 10px; padding-left: 20px;" title="Mohon untuk melengkapi form pencarian terlebih dahulu!" disabled><span class=""></span> Submit</button>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>



@section scripts{
    <script src="~/Scripts/Admin/JadwalKuliah/search.js"></script>
    <script src="~/Scripts/Admin/JadwalKuliah/main_manage_matakuliah.js"></script>
    <script src="~/Scripts/loading/loadingoverlay.min.js"></script>
}