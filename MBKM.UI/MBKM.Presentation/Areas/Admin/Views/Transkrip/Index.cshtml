
@{
    ViewBag.Title = "Transkrip Nilai Seluruh Mahasiswa";
    Layout = "~/Areas/Admin/Views/Shared/_Layout2.cshtml";
}

@section css{

    <link rel="stylesheet" type="text/css" href="~/Content/Admin/inputResponsive.css">
    @Scripts.Render("~/bundles/jquery")


    @Scripts.Render("~/bundles/jquery")
    <script src="~/Scripts/sweetalert2.all.min.js"></script>
}


<div class="container-fluid" style="padding: 0px">
    <div class="card center-block" style="background-color: white; width: 100%; padding: 0px">
        <div class="card-body" style="box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2); border: 3px solid coral; padding: 0px ">
            <h4 class="card-title" style="padding-top: 30px; padding-right: 20px; padding-left: 30px; font-weight: bold; font-size: 30px; color: black;">
                TRANSKRIP NILAI SELURUH MAHASISWA
            </h4>
            <hr />
            <div class="container" style="width: 100%;">
                <br />
                <br />
                <div id="table-data-transkrip-overflow">
                    <table id="table-data-transkrip" class="table table-bordered table-spacing js-basic-example dataTable display nowrap">
                        <thead>
                            <tr>
                                <th class="zero-padding no-border vertical-center sorting">Action</th>
                                <th class="zero-padding no-border vertical-center sorting">No.</th>
                                <th class="zero-padding no-border vertical-center sorting">Jenjang Studi</th>
                                <th class="zero-padding no-border vertical-center sorting">Universitas Asal</th>
                                <th class="zero-padding no-border vertical-center sorting">NIM</th>
                                <th class="zero-padding no-border vertical-center sorting">Name Mahasiswa</th>
                                <th class="zero-padding no-border vertical-center sorting">No Kerjasama</th>
                                <th class="zero-padding no-border vertical-center sorting">Status Cetak</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>


            <div id="print" style="display:none">
                <style>
                    .btn{color:#fff!important}
                    .btn-default{color:rgba(0,0,0,0.2)!important}

                    .box-print-foot {
                        border-bottom: 1px solid
                    }

                    .box-print {
                        padding: 10px 20px;
                        border-top: solid 1px;
                        border-right: solid 1px;
                        border-left: solid 1px;
                    }

                    .en {
                        padding-left: 10px;
                        font-size: 12px;
                    }

                    .no-padding {
                        padding: 0px !important
                    }

                    .header div {
                        line-height: 22px;
                    }

                    td {
                        vertical-align: top;
                    }
                </style>
